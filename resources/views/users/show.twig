{% extends "layouts/base.twig" %}

{% block content %}
    {% if query is not empty %}
    <div>
        <p>
            My name is {{query.getName}}
        </p>
    </div>
    <div>
        <p>
            My ID is {{query.getId}}
        </p>
    </div>
    {% else %}
    <p>
        Users with this ID does not exist.
    </p>
    {% endif %}
{% endblock %}